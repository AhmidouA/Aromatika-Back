{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/user(.*)",
          "dest": "index.js"
        },
        {
          "src": "/oil(.*)",
          "dest": "index.js"
        },
        {
          "src": "/category(.*)",
          "dest": "index.js"
        },
        {
          "src": "/family(.*)",
          "dest": "index.js"
        },
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
      ]
    }
